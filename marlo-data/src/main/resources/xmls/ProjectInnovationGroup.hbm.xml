<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.cgiar.ccafs.marlo.data.model.ProjectInnovationGroup" table="project_innovation_group" optimistic-lock="none">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="projectInnovationOwner" type="java.lang.Long">
          <column name="project_innovation_owner" />
        </property>
        <many-to-one name="projectInnovation" class="org.cgiar.ccafs.marlo.data.model.ProjectInnovation" fetch="select">
            <column name="project_innovation_id" />
        </many-to-one>
        <many-to-one name="phase" class="org.cgiar.ccafs.marlo.data.model.Phase" fetch="select">
            <column name="id_phase" />
        </many-to-one>        
    </class>
</hibernate-mapping>